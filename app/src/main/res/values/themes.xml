<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="GrayThemeProject" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">?brandColor_colorPrimary</item>
        <item name="colorPrimaryVariant">?brandColor_colorPrimaryVariant</item>
        <item name="colorOnPrimary">?brandColor_colorOnPrimary</item>
        <item name="android:navigationBarColor">?brandColor_navigationBarColor</item>
        <item name="colorPrimaryContainer">?brandColor_colorPrimaryContainer</item>
        <item name="colorOnPrimaryContainer">?brandColor_colorOnPrimaryContainer</item>
        <item name="colorPrimaryInverse">?brandColor_colorPrimaryInverse</item>
        <item name="colorSecondary">?brandColor_colorSecondary</item>
        <item name="colorOnSecondary">?brandColor_colorOnSecondary</item>
        <item name="colorSecondaryContainer">?brandColor_colorSecondaryContainer</item>
        <item name="colorOnSecondaryContainer">?colorOnSecondaryContainer</item>
        <item name="colorTertiary">?brandColor_colorTertiary</item>
        <item name="colorOnTertiary">?brandColor_colorOnTertiary</item>
        <item name="colorTertiaryContainer">?brandColor_colorTertiaryContainer</item>
        <item name="colorOnTertiaryContainer">?brandColor_colorOnTertiaryContainer</item>
        <item name="android:background">?brandColor_background</item>
        <item name="colorOnBackground">?brandColor_onBackground</item>
        <!-- Secondary brand color. -->

        <item name="colorSecondaryVariant">@color/teal_700</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <item name="chipStyle">@style/BlueGreyStyleChip</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="TealThemeProject" parent="GrayThemeProject">
        <item name="brandColor">@color/teal_900</item>
        <item name="brandColor_colorPrimary">@color/teal_theme_light_primary</item>
        <item name="brandColor_colorSecondary">@color/teal_theme_light_secondary</item>
        <item name="brandColor_colorPrimaryVariant">@color/teal_theme_light_primaryVariant</item>
        <item name="brandColor_colorOnPrimary">@color/teal_theme_light_onPrimary</item>
        <item name="brandColor_navigationBarColor">@color/teal_theme_light_navigationBarColor</item>
        <item name="brandColor_colorPrimaryContainer">@color/teal_theme_light_primaryContainer</item>
        <item name="brandColor_colorOnPrimaryContainer">@color/teal_theme_light_onPrimaryContainer</item>
        <item name="brandColor_colorPrimaryInverse">@color/teal_theme_light_colorPrimaryInverse</item>
        <item name="brandColor_colorOnSecondary">@color/teal_theme_light_onSecondary</item>
        <item name="brandColor_colorSecondaryContainer">@color/teal_theme_light_secondaryContainer</item>
        <item name="brandColor_colorOnSecondaryContainer">@color/teal_theme_light_onSecondaryContainer</item>
        <item name="brandColor_colorTertiary">@color/teal_theme_light_tertiary</item>
        <item name="brandColor_colorOnTertiary">@color/teal_theme_light_onTertiary</item>
        <item name="brandColor_colorTertiaryContainer">@color/teal_theme_light_tertiaryContainer</item>
        <item name="brandColor_colorOnTertiaryContainer">@color/teal_theme_light_onTertiaryContainer</item>
        <item name="brandColor_background">@color/teal_theme_light_background</item>
        <item name="brandColor_onBackground">@color/teal_theme_light_onBackground</item>


    </style>

    <style name="MagentaThemeProject" parent="GrayThemeProject">
        <item name="brandColor">@color/dark_moderate_magenta</item>
        <item name="brandColor_colorPrimary">@color/md_theme_light_primary</item>
        <item name="brandColor_colorSecondary">@color/md_theme_light_secondary</item>
        <item name="brandColor_colorPrimaryVariant">@color/md_theme_light_primaryVariant</item>
        <item name="brandColor_colorOnPrimary">@color/md_theme_light_onPrimary</item>
        <item name="brandColor_navigationBarColor">@color/md_theme_light_navigationBarColor</item>
        <item name="brandColor_colorPrimaryContainer">@color/md_theme_light_primaryContainer</item>
        <item name="brandColor_colorOnPrimaryContainer">@color/md_theme_light_onPrimaryContainer</item>
        <item name="brandColor_colorPrimaryInverse">@color/md_theme_light_colorPrimaryInverse</item>
        <item name="brandColor_colorOnSecondary">@color/md_theme_light_onSecondary</item>
        <item name="brandColor_colorSecondaryContainer">@color/md_theme_light_secondaryContainer</item>
        <item name="brandColor_colorOnSecondaryContainer">@color/md_theme_light_onSecondaryContainer</item>
        <item name="brandColor_colorTertiary">@color/md_theme_light_tertiary</item>
        <item name="brandColor_colorOnTertiary">@color/md_theme_light_onTertiary</item>
        <item name="brandColor_colorTertiaryContainer">@color/md_theme_light_tertiaryContainer</item>
        <item name="brandColor_colorOnTertiaryContainer">@color/md_theme_light_onTertiaryContainer</item>
        <item name="brandColor_background">@color/md_theme_light_background</item>
        <item name="brandColor_onBackground">@color/md_theme_light_onBackground</item>
    </style>


</resources>