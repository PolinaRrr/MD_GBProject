        <item name="brandColor_colorPrimary">@color/teal_theme_dark_primary</item>
        <item name="brandColor_colorSecondary">@color/teal_theme_dark_secondary</item>
        <item name="brandColor_colorPrimaryVariant">@color/teal_theme_dark_primaryVariant</item>
        <item name="brandColor_colorOnPrimary">@color/teal_theme_dark_onPrimary</item>
        <item name="brandColor_navigationBarColor">@color/teal_theme_dark_navigationBarColor</item>
        <item name="brandColor_colorPrimaryContainer">@color/teal_theme_dark_primaryContainer</item>
        <item name="brandColor_colorOnPrimaryContainer">@color/teal_theme_dark_onPrimaryContainer</item>
        <item name="brandColor_colorPrimaryInverse">@color/teal_theme_dark_colorPrimaryInverse</item>
        <item name="brandColor_colorOnSecondary">@color/teal_theme_dark_onSecondary</item>
        <item name="brandColor_colorSecondaryContainer">@color/teal_theme_dark_secondaryContainer</item>
        <item name="brandColor_colorOnSecondaryContainer">@color/teal_theme_dark_onSecondaryContainer</item>
        <item name="brandColor_colorTertiary">@color/teal_theme_dark_tertiary</item>
        <item name="brandColor_colorOnTertiary">@color/teal_theme_dark_onTertiary</item>
        <item name="brandColor_colorTertiaryContainer">@color/teal_theme_dark_tertiaryContainer</item>
        <item name="brandColor_colorOnTertiaryContainer">@color/teal_theme_dark_onTertiaryContainer</item>
        <item name="brandColor_background">@color/teal_theme_dark_background</item>
        <item name="brandColor_onBackground">@color/teal_theme_dark_onBackground</item>
    </style>

