        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/blue_gray_600</item>
        <item name="colorPrimaryVariant">@color/blue_gray_300</item>
        <item name="colorOnPrimary">@color/blue_gray_700</item>
        <item name="android:navigationBarColor">@color/blue_gray_500</item>
        <item name="colorPrimaryContainer">@color/blue_gray_800</item>
        <item name="colorOnPrimaryContainer">@color/blue_gray_700</item>
        <item name="colorPrimaryInverse">@color/blue_gray_800</item>
        <item name="colorSecondary">@color/blue_gray_900</item>
        <item name="colorOnSecondary">@color/blue_gray_800</item>
        <item name="colorSecondaryContainer">@color/blue_gray_800</item>
        <item name="colorOnSecondaryContainer">@color/blue_gray_700</item>
        <item name="colorTertiary">@color/blue_gray_600</item>
        <item name="colorOnTertiary">@color/black</item>
        <item name="colorTertiaryContainer">@color/blue_gray_800</item>
        <item name="colorOnTertiaryContainer">@color/blue_gray_300</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>